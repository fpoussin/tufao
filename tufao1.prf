QT += network

DEFINES += TUFAO_VERSION_MAJOR=1
INCLUDEPATH += $$[QT_INSTALL_HEADERS]/QtTufao/ $$[QT_INSTALL_HEADERS]/QtTufao/1.4.4/QtTufao $$[QT_INSTALL_HEADERS]/QtTufao/1.4.4/QtTufao/private

msvc {
  CONFIG(debug, debug|release): LIBS += -lQtTufaod1
  CONFIG(release, debug|release): LIBS += -lQtTufao1
}
else {
  CONFIG(debug, debug|release): LIBS += -lQtTufaod
  CONFIG(release, debug|release): LIBS += -lQtTufao
}
